<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">

<ui:UiBinder xmlns:ui='urn:ui:com.google.gwt.uibinder'
	xmlns:g='urn:import:com.google.gwt.user.client.ui'
	ui:generateFormat='com.google.gwt.i18n.rebind.format.PropertiesFormat'
	ui:generateKeys='com.google.gwt.i18n.rebind.keygen.MD5KeyGenerator'
	ui:generateLocales='default'>

	<g:PopupPanel styleName="processwindow">
	    <g:HTMLPanel>
		    <span ui:field="spnTitle" style="font-size:14px; font-weight:bold; color:orange; text-decoration:underline"></span>
		    <g:HTMLPanel>
		        <span ui:field="spnDescription"></span>
		    </g:HTMLPanel>
		        
		    <g:HTMLPanel ui:field="parameterPanel">		        
			    <g:FlexTable ui:field="flexTable">	        
			    </g:FlexTable>
		    </g:HTMLPanel>
		    <div>
			    <g:Button ui:field="btnExecute">Ok</g:Button>
			    <g:Button ui:field="btnCancel">Cancel</g:Button>
		    </div>
	    </g:HTMLPanel>
	</g:PopupPanel>
</ui:UiBinder>
